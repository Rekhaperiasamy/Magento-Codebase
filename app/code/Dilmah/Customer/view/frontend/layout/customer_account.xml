<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" label="Customer My Account (All Pages)" design_abstraction="custom">
    <body>
        <referenceBlock name="autoresponder_customer_list" remove="true"/>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\GiftRegistry\Block\Link" name="customer-account-change-password-link" after="customer-account-navigation-reward-link">
                <arguments>
                    <argument name="path" xsi:type="string">dilmah_customer/password</argument>
                    <argument name="label" xsi:type="string">Change Password</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
