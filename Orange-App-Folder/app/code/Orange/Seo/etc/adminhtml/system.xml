    <config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
        <system>
            <section id="googlefeed_sec" translate="label" sortOrder="130" showInDefault="1" showInWebsite="1" showInStore="1">
                <class>separator-top</class>
                <label>Google Feed</label>
                <tab>orange</tab>
                <resource>Orange_Seo::googlefeed_configuration</resource>
        
                <group id="googlefeed_configuration" translate="label" type="text" sortOrder="3" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Setting</label>
                    <field id="attributelist" translate="label" type="multiselect" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1">
                        <label>Select Attribute Set</label>
                        <source_model>Orange\Seo\Model\Config\Source\Options</source_model>
                    </field>
                </group>
            </section>
            <section id="fraudreport_ftpdetails" translate="label" sortOrder="130" showInDefault="1" showInWebsite="1" showInStore="1">
                <class>separator-top</class>
                <label>Fraud Report FTP Details</label>
                <tab>orange</tab>
                <resource>Orange_Seo::frouddetailftp_configuration</resource>
                
                <group id="fraudreport__configuration" translate="label" type="text" sortOrder="3" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Froud Report FTP Details</label>
                    <field id="server_detail" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1">
                        <label>Server Detail:</label>
                    </field>
                    <field id="server_user_id" translate="label" type="text" sortOrder="11" showInDefault="1" showInWebsite="1" showInStore="1">
                        <label>User:</label>
                    </field>
                    <field id="server_pwd" translate="label" type="password" sortOrder="12" showInDefault="1" showInWebsite="1" showInStore="1">
                        <label>Password:</label>
                    </field>
                    <field id="server_ppkey" translate="label" type="Magento\Config\Block\System\Config\Form\Field\File" sortOrder="13" showInDefault="1" showInWebsite="1" >
                        <label>Server Key</label>
                        <backend_model>Magento\Config\Model\Config\Backend\File</backend_model>
                        <upload_dir config="system">upload</upload_dir>
                    </field>
                    <field id="server_directorypath" translate="label" type="text" sortOrder="14" showInDefault="1" showInWebsite="1" showInStore="1">
                        <label>Directory Path:</label>
                    </field>
                </group>
            </section>
            <section id="samsung_ftpdetails" translate="label" sortOrder="150" showInDefault="1" showInWebsite="1" showInStore="1">
                <class>separator-top</class>
                <label>Samsung Report FTP Details</label>
                <tab>orange</tab>
                <resource>Orange_Seo::samsungreportftp_configuration</resource>
                <group id="samsungdetail__configuration" translate="label" type="text" sortOrder="3" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Samsung Report Report FTP Details</label>
                    <field id="ftp_server" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1">
                        <label>FTP Server:</label>
                    </field>
                    <field id="ftp_user_name" translate="label" type="text" sortOrder="11" showInDefault="1" showInWebsite="1" showInStore="1">
                        <label>User:</label>
                    </field>
                    <field id="ftp_user_pass" translate="label" type="password" sortOrder="12" showInDefault="1" showInWebsite="1" showInStore="1">
                        <label>Password:</label>
                    </field>
                </group>
            </section>
			 <section id="bireport_ftpdetails" translate="label" sortOrder="150" showInDefault="1" showInWebsite="1" showInStore="1">
                <class>separator-top</class>
                <label>BI Report FTP Details</label>
                <tab>orange</tab>
                <resource>Orange_Seo::bireportftp_configuration</resource>
                <group id="bidetail__configuration" translate="label" type="text" sortOrder="3" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>BI Report Report FTP Details</label>
                <field id="bireport_host" translate="label" type="text" sortOrder="23" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>BI Report Host</label>
                </field>
				<field id="bireport_username" translate="label" type="text" sortOrder="24" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>BI Report Username</label>
                </field>
				<field id="bireport_key" translate="label" type="Magento\Config\Block\System\Config\Form\Field\File" sortOrder="25" showInDefault="1" showInWebsite="1" >
					<label>BI Report Password</label>
					<backend_model>Magento\Config\Model\Config\Backend\File</backend_model>
					<upload_dir config="system">upload</upload_dir>
				</field>
				<field id="bireport_path" translate="label" type="text" sortOrder="26" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>BI Report Path</label>
                </field>
                </group>
            </section>
            <section id="custom_modulelog" translate="label" sortOrder="180" showInDefault="1" showInWebsite="1" showInStore="1">
                <class>separator-top</class>
                <label>Custom Module Log Setting</label>
                <tab>orange</tab>
                <resource>Orange_Seo::modulelog_configuration</resource>
                <group id="modulelog__configuration" translate="label" type="text" sortOrder="3" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Custom Module Log Setting</label>
                    <field id="setting_mode" translate="label" type="select" sortOrder="20" showInDefault="1" showInWebsite="1" showInStore="1">
                        <label>Enable module logging</label>
                        <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                    </field> 
                </group>
            </section>
			<section id="productsearch_ftpdetails" translate="label" sortOrder="210" showInDefault="1" showInWebsite="1" showInStore="1">
                <class>separator-top</class>
                <label>Product Search FTP Details</label>
                <tab>orange</tab>
                <resource>Orange_Seo::productsearchftp_configuration</resource>
                <group id="productsearch__configuration" translate="label" type="text" sortOrder="3" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Product Search FTP Details</label>
                    <field id="productsearch_ftp_server" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1">
                        <label>Host:</label>
                    </field>
					<field id="productsearch_ftp_path" translate="label" type="text" sortOrder="11" showInDefault="1" showInWebsite="1" showInStore="1">
                        <label>Path:</label>
                    </field>
                    <field id="productsearch_user_name" translate="label" type="text" sortOrder="12" showInDefault="1" showInWebsite="1" showInStore="1">
                        <label>User Name:</label>
                    </field>
                    <field id="productsearch_user_pass" translate="label" type="password" sortOrder="13" showInDefault="1" showInWebsite="1" showInStore="1">
                        <label>Password:</label>
                    </field>
                </group>
            </section>
                <section id="reevoo_product_export" translate="label" sortOrder="210" showInDefault="1" showInWebsite="1" showInStore="1">
                <class>separator-top</class>
                <label>Reevoo Product Export</label>
                <tab>orange</tab>
                <resource>Orange_Seo::reevooproductexport_configuration</resource>
                <group id="reevooauth__configuration" translate="label" type="text" sortOrder="3" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Reevoo Product Export - Authenticate Credential</label>
                    <field id="reevooauth_user_name" translate="label" type="text" sortOrder="12" showInDefault="1" showInWebsite="1" showInStore="1">
                        <label>User Name:</label>
                    </field>
                    <field id="reevooauth_user_pass" translate="label" type="text" sortOrder="13" showInDefault="1" showInWebsite="1" showInStore="1">
                        <label>Password:</label>
                    </field>
                </group>
            </section>
				
        </system>
    </config>


