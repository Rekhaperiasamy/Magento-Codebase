<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="home.spotlight.wrapper">
            <block class="Magento\Cms\Block\Block" name="home.spotlight" >
                <arguments>
                    <argument name="block_id" xsi:type="string">home_spotlights</argument>
                </arguments>
            </block>
        </referenceContainer>
        <referenceContainer name="seo.text.content.wrapper">
            <block class="Magento\Framework\View\Element\Template" name="seo.content.wrap" template="Dilmah_Theme::html/seocontent.phtml">
                <block class="Magento\Cms\Block\Block" name="seo.title" >
                    <arguments>
                        <argument name="block_id" xsi:type="string">home_seo_title</argument>
                    </arguments>
                </block>
                <block class="Magento\Cms\Block\Block" name="seo.content" >
                    <arguments>
                        <argument name="block_id" xsi:type="string">home_seo_content</argument>
                    </arguments>
                </block>
            </block>
        </referenceContainer>
    </body>
</page>
